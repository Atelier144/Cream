<%= form_tag("/auth/github/callback/done", class: "callback-form") do %>
  <h1>ただいま、GitHubと連携中です</h1>
  <label><input type="text" name="provider" value="<%= @provider %>"></label>
  <label><input type="text" name="uid" value="<%= @uid %>"></label>
  <label><input type="text" name="name" value="<%= @name %>"></label>
  <label><input type="text" name="image" value="<%= @image %>"></label>
  <label><input type="text" name="description" value="<%= @description %>"></label>
  <label><input type="text" name="url" value="<%= @url %>"></label>
  <label><input type="text" name="github_url" value="<%= @github_url %>"></label>
  <label><input type="text" name="location" value="<%= @location %>"></label>
  <input type="submit" value="こちら">
<% end %>